{
    "name": "homebridge-esphome-ts",
    "version": "1.0.1",
    "description": "New plugin for integrating for esphome into homebridge",
    "main": "dist/index.js",
    "scripts": {
        "format": "npx eslint --fix src/**/*.ts",
        "prepack": "npm run format && npm run cbuild",
        "build": "npx tsc",
        "test": "npx jest",
        "clean": "rm -rf dist",
        "cbuild": "npm run clean && npm run build"
    },
    "repository": "https://github.com/lucavb/homebridge-esphome-ts",
    "files": [
        "/dist",
        "config.schema.json"
    ],
    "keywords": [
        "homebridge",
        "esphome",
        "homebridge-plugin"
    ],
    "author": "Luca Becker <luca.becker@sunbury.xyz> (https://sunbury.xyz/)",
    "license": "GPL-3.0",
    "engines": {
        "node": ">=0.12.0",
        "homebridge": ">=0.4.0"
    },
    "dependencies": {
        "color-convert": "^2.0.1",
        "esphome-ts": "^2.1.5",
        "protobufjs": "^6.9.0",
        "rxjs": "^6.5.5"
    },
    "devDependencies": {
        "@types/color-convert": "^1.9.0",
        "@types/node": "^14.14.16",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "eslint": "^6.8.0",
        "eslint-config-google": "^0.14.0",
        "hap-nodejs": "^0.7.3",
        "homebridge": "^1.1.0",
        "typescript": "^3.9.5"
    }
}
