{
    "env": {
        "es2021": true,
        "node": true
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "extends": ["google", "plugin:prettier/recommended"],
    "ignorePatterns": ["dist"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "eqeqeq": "error",
        "prettier/prettier": ["error"],
        "require-jsdoc": "off",
        "no-unused-vars": "off"
    }
}
